{
  "name": "simple-family-tree-gedcom-decoder",
  "version": "1.0.10",
  "description": "Gedcom file decoder for simple-family-tree-model family tree",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "files": [
    "lib/src/*"
  ],
  "scripts": {
    "clean": "npx rimraf ./lib/src/ && npx rimraf ./lib/tests/ ",
    "build": "tsc -p tsconfig.json",
