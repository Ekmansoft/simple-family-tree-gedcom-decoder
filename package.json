    "lint": "eslint --ext .tsx ./src/"
  },
  "keywords": [
    "genealogy",
    "tree",
    "family-tree"
  ],
  "author": "Kenneth Ekman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ekmansoft/simple-family-tree-gedcom-decoder"
  },
  "dependencies": {
