    "@types/node": "^18.7.23",
    "axios": "^1.1.2",
    "gedcom": "^3.0.0",
    "simple-family-tree-model": "^0.3.2"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "chai": "^4.3.6",
    "eslint": "^8.24.0",
    "esm-utils": "^4.1.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
